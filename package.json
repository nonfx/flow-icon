{
  "name": "@cldcvr/flow-icon",
  "version": "0.0.4",
  "description": "This package contains free set icons to use in `@cldcvr/flow-core`",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "workspaces": [
    "packages/@cldcvr/*",
    "packages/storybook"
  ],
  "scripts": {
    "start": "yarn workspace icon-storybook start",
    "build-stories": "yarn workspace icon-storybook build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-packages": "ts-node-esm package-builder/create-packages.ts && yarn",
    "build-packages": "yarn workspaces run build",
    "release": "yarn build-packages && yarn changeset publish",
    "version": "yarn update-packages && yarn changeset version"
  },
  "keywords": [
    "icons"
  ],
  "author": "@cldcvr",
  "license": "MIT",
  "dependencies": {
    "@cldcvr/flow-core": "*"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@types/node": "^18.7.17",
    "@types/prettier": "^2.7.0",
    "axios": "^0.27.2",
    "fs": "^0.0.1-security",
    "lit-html": "^2.2.7",
    "prettier": "^2.7.1",
    "semver": "^7.3.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "rollup-plugin-postcss-lit": "^2.0.0",
    "rollup-plugin-scss": "3.0.0"
  },
  "peerDependencies": {
    "@cldcvr/flow-core": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/cldcvr/flow-icon.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
